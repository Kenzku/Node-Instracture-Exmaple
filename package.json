{
  "name": "Node-UI-Infrasture",
  "private": true,
  "version": "0.0.1",
  "description": "Node based using reactJS, webpack, expressJS and Stylus",
  "main": "./server/bin/www",
  "engines": {
    "node": "^8.3.0",
    "npm": "^5.3.0"
  },
  "scripts": {
    "postinstall": "copyfiles -f git_hooks/pre-push .git/hooks/ && copyfiles -f git_hooks/pre-commit .git/hooks/",
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./client/src/real_index.html ./client/src/favicon.ico ./dist",
    "copy_dev": "copyfiles -f ./client/src/favicon.ico ./dist",
    "zip": "zip --symlinks -r dist/production_build.zip * . -x '*.git*'",
    "build": "npm run clean && npm run copy & webpack --env=production --progress && npm run zip",
    "prod": "npm run clean && npm run copy & webpack --env=production --progress",
    "lint": "eslint ./client/src",
    "lint:preCommit": "eslint ./client/src --max-warnings 1",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "start:prod": "node ./server/bin/www --env=production",
    "start": "npm run copy_dev && node ./server/bin/www --env=dev",
    "start:pm2": "npm run copy_dev && pm2 start ./server/bin/www -i 0",
    "stop:pm2": "pm2 delete all",
    "start:pm2:prod": "pm2 start ./server/bin/www -i 0 --env production",
    "list": "pm2 list",
    "reload": "pm2 reload all",
    "monitor": "pm2 monit"
  },
  "repository": "",
  "keywords": [],
  "author": "Huang Fuguo (aka Ken)",
  "contributors": [
    "Huang Fuguo(aka Ken): fuguo.huang@ie.ibm.com"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^7.2.1",
    "faker": "^4.1.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "babel-register": "^6.24.1",
    "body-parser": "~1.17.2",
    "compression": "^1.6.2",
    "debug": "^3.0.0",
    "express": "^4.13.4",
    "express-session": "^1.14.0",
    "ignore-styles": "^5.0.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "js-yaml": "^3.9.1",
    "keymirror": "^0.1.1",
    "minimist": "^1.2.0",
    "normalize.css": "^7.0.0",
    "object-hash": "^1.1.3",
    "pm2": "^2.6.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-router-dom": "^4.1.2",
    "request": "^2.81.0",
    "rxjs": "^5.4.3",
    "serve-favicon": "^2.3.0",
    "socket.io": "2.0.3",
    "svgxuse": "^1.1.16",
    "winston": "^2.2.0"
  }
}
